<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.jeff.darknotify</string>

    <key>ProgramArguments</key>
    <array>
        <string>/opt/homebrew/bin/dark-notify</string>
        <string>-c</string>
        <string>/Users/jeff/.config/dark-mode-switcher/switch_theme</string>
    </array>


    <key>StandardOutPath</key>
    <string>/tmp/darknotify/output.log</string>

    <key>StandardErrorPath</key>
    <string>/tmp/darknotify/error.log</string>


    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>
</dict>
</plist>
